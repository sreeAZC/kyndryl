pipeline {

    agent any
        stages {
                stage('build') {
                        steps {
                                sh 'echo "Building application"'
                        }
                
                     }

                  stage('test') {
                        steps {
                                sh 'echo "test application" '
                        }
                
                     }

                stage('deploy') {
                        steps {
                                sh 'echo "deploying application" '
                        }
                
                     }

    }
       }
